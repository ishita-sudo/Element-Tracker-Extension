{
  "manifest_version": 3,
  "name": "Element Inspector",
  "version": "1.0",
  "description": "Tracks HTML elements on web pages.",
  "permissions": ["scripting","storage","tabs"],
  "host_permissions": ["<all_urls>"],
  "action":{
    "default_popup":"popup.html"
   
  },
  "background": {
    "service_worker": "background.js"
  }
}
